{
	"name": "@raruto/leaflet-elevation",
	"version": "2.2.2",
	"description": "A Leaflet plugin that allows to add elevation profiles using d3js",
	"type": "module",
	"main": "dist/leaflet-elevation.min.js",
	"browser": "dist/leaflet-elevation.min.js",
	"module": "src/index.js",
	"scripts": {
