		"build": "rollup -c build/rollup.config.js",
		"watch": "rollup -c build/rollup.config.js --watch",
		"dev": "npm run watch",
		"version": "npm run build && git add -A"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Raruto/leaflet-elevation.git"
	},
	"keywords": [
