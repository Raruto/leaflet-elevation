		"elevation",
		"highchart",
		"kml",
		"altitude",
		"gpx",
		"gps-visualizer",
		"tcx"
	],
	"author": {
		"name": "Felix “MrMufflon” Bache",
		"url": "https: //github.com/MrMufflon/Leaflet.Elevation"
	},
	"contributors": [
